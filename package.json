{
  "name": "fboat-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "APP_ENV=dev node dist/main",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "build:watch": "npm run clean && tsc -p tsconfig-build.json -w",
    "clean": "rimraf dist",
    "dev": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main",
    "test": "APP_ENV=test jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "npm test -- --watch",
    "test:watch:changed": "npm test -- --watch --onlyChanged",
    "test:integration": "npm test -- --config ./jest.integration.config.ts",
    "test:coverage": "npm t -- --coverage --config ./jest.coverage.config.ts",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "typeorm": "typeorm-ts-node-esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msalvexx/fboat-backend.git"
  },
