    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
