  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.21.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
